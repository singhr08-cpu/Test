      ],
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "def calculate_profit(sales_price, coop_beans_pounds, open_market_beans_pounds, coop_bean_yield):\n",
        "  \"\"\"Calculates the profit from selling cocoa powder.\n",
        "\n",
        "  Args:\n",
        "    sales_price: The selling price of cocoa powder per pound.\n",
        "    coop_beans_pounds: The quantity of beans purchased from cooperatives in pounds.\n",
        "    open_market_beans_pounds: The quantity of beans purchased from the open market in pounds.\n",
        "    coop_bean_yield: The yield of cocoa powder from coop beans (as a decimal between 0 and 1).\n",
        "\n",
        "  Returns:\n",
        "    The total profit in dollars.\n",
        "  \"\"\"\n",
        "  COOP_BEAN_PRICE = 1.5  # $/pound\n",
        "  OPEN_MARKET_BEAN_PRICE = 1.75  # $/pound\n",
        "  IN_HOUSE_FIXED_COSTS = 100e6  # $\n",
        "  IN_HOUSE_VARIABLE_COSTS = 4.3  # $/pound\n",
        "  OUTSOURCED_VARIABLE_COSTS = 5.3  # $/pound\n",
        "  IN_HOUSE_CAPACITY = 200e6  # pounds\n",
        "\n",
        "  coop_powder_pounds = coop_beans_pounds * coop_bean_yield\n",
        "  total_beans_processed = coop_beans_pounds + open_market_beans_pounds\n",
        "\n",
        "  outsourced_beans_pounds = max(0, total_beans_processed - IN_HOUSE_CAPACITY)\n",
        "  in_house_beans_pounds = total_beans_processed - outsourced_beans_pounds\n",
        "\n",
        "  in_house_processing_cost = IN_HOUSE_FIXED_COSTS + in_house_beans_pounds * IN_HOUSE_VARIABLE_COSTS\n",
        "  outsourced_processing_cost = outsourced_beans_pounds * OUTSOURCED_VARIABLE_COSTS\n",
        "\n",
        "  total_cost = (\n",
        "      calculate_procurement_cost(coop_beans_pounds, open_market_beans_pounds)\n",
        "      + in_house_processing_cost\n",
        "      + outsourced_processing_cost\n",
        "  )\n",
        "\n",
        "  demand = 600e6 - 35e6 * sales_price  # in pounds\n",
        "  total_revenue = min(demand, coop_powder_pounds + open_market_beans_pounds) * sales_price\n",
        "\n",
        "  profit = total_revenue - total_cost\n",
        "  return profit\n",
        "\n",
        "def calculate_procurement_cost(coop_beans_pounds, open_market_beans_pounds):\n",
        "  \"\"\"Calculates the total cost of procuring cocoa beans.\n",
        "\n",
        "  Args:\n",
        "    coop_beans_pounds: The quantity of beans purchased from cooperatives in pounds.\n",
        "    open_market_beans_pounds: The quantity of beans purchased from the open market in pounds.\n",
        "\n",
        "  Returns:\n",
        "    The total procurement cost in dollars.\n",
        "  \"\"\"\n",
        "  COOP_BEAN_PRICE = 1.5  # $/pound\n",
        "  OPEN_MARKET_BEAN_PRICE = 1.75  # $/pound\n",
        "\n",
        "  coop_cost = coop_beans_pounds * COOP_BEAN_PRICE\n",
        "  open_market_cost = open_market_beans_pounds * OPEN_MARKET_BEAN_PRICE\n",
        "  total_cost = coop_cost + open_market_cost\n",
        "  return total_cost\n",
        "\n",
        "def calculate_gross_profit(sales_price, coop_bean_yield=0.85, coop_contract_proportion=0.6, open_market_price=1.75):\n",
        "  \"\"\"Calculates Marx's gross profit based on various parameters.\n",
        "\n",
        "  Args:\n",
        "    sales_price: The selling price of cocoa powder per pound.\n",
        "    coop_bean_yield: The yield of cocoa powder from coop beans.\n",
        "    coop_contract_proportion: The proportion of initial beans quantity procured through the coop contract.\n",
        "    open_market_price: The price of beans in the open market.\n",
        "\n",
        "  Returns:\n",
        "    Marx's gross profit in dollars.\n",
        "  \"\"\"\n",
        "  # Calculate demand for cocoa powder\n",
        "  demand = 600e6 - 35e6 * sales_price\n",
        "\n",
        "  # Calculate initial quantity of coop beans needed based on demand, BEFORE considering yield\n",
        "  initial_coop_beans = demand * coop_contract_proportion\n",
        "\n",
        "  # Account for yield loss in coop beans to get the actual amount of coop beans to purchase\n",
        "  actual_coop_beans = initial_coop_beans / coop_bean_yield\n",
        "\n",
        "  # Calculate the amount of open market beans needed to meet the remaining demand\n",
        "  open_market_beans = demand - (actual_coop_beans * coop_bean_yield)\n",
        "\n",
        "  profit = calculate_profit(sales_price, actual_coop_beans, open_market_beans, coop_bean_yield)\n",
        "  return profit\n",
        "\n",
        "def calculate_sourcing_percentages(sales_price, coop_bean_yield=0.85, coop_contract_proportion=0.6, open_market_price=1.75):\n",
        "  \"\"\"Calculates the percentage of cocoa sourced from the open market and coop.\n",
        "\n",
        "  Args:\n",
        "    sales_price: The selling price of cocoa powder per pound.\n",
        "    coop_bean_yield: The yield of cocoa powder from coop beans.\n",
        "    coop_contract_proportion: The proportion of initial beans quantity procured through the coop contract.\n",
        "    open_market_price: The price of beans in the open market.\n",
        "\n",
        "  Returns:\n",
        "    A tuple containing the percentage of cocoa sourced from the open market and the percentage sourced from the coop.\n",
        "  \"\"\"\n",
        "  # Calculate demand for cocoa powder\n",
        "  demand = 600e6 - 35e6 * sales_price\n",
        "\n",
        "  # Calculate initial quantity of coop beans needed based on demand, BEFORE considering yield\n",
        "  initial_coop_beans = demand * coop_contract_proportion\n",
        "\n",
        "  # Account for yield loss in coop beans to get the actual amount of coop beans to purchase\n",
        "  actual_coop_beans = initial_coop_beans / coop_bean_yield\n",
        "\n",
        "  # Calculate the amount of open market beans needed to meet the remaining demand\n",
        "  open_market_beans = demand - (actual_coop_beans * coop_bean_yield)\n",
        "\n",
        "  total_beans = actual_coop_beans + open_market_beans\n",
        "  open_market_percentage = (open_market_beans / total_beans) * 100\n",
        "  coop_percentage = (actual_coop_beans / total_beans) * 100\n",
        "\n",
        "  return open_market_percentage, coop_percentage\n",
        "\n",
        "# Generate data for the plot\n",
        "prices = np.arange(8, 15.25, 0.05)\n",
        "profits = []\n",
        "open_market_percentages = []\n",
        "\n",
        "for price in prices:\n",
        "    profit = calculate_gross_profit(price)\n",
        "    profits.append(profit)\n",
        "\n",
        "    (open_market_percentage, _) = calculate_sourcing_percentages(price)\n",
        "    open_market_percentages.append(open_market_percentage)\n",
        "\n",
        "# Create the plot with two y-axes\n",
        "fig, ax1 = plt.subplots()\n",
        "\n",
        "color = 'tab:red'\n",
        "ax1.set_xlabel(\"Price of Cocoa Powder ($/pound)\")\n",
        "ax1.set_ylabel(\"Gross Profit ($)\", color=color)\n",
        "ax1.plot(prices, profits, color=color)\n",
        "ax1.tick_params(axis='y', labelcolor=color)\n",
        "\n",
        "ax2 = ax1.twinx()  # instantiate a second axes that shares the same x-axis\n",
        "\n",
        "color = 'tab:blue'\n",
        "ax2.set_ylabel('% Sourced from Open Market', color=color)\n",
        "ax2.plot(prices, open_market_percentages, color=color)\n",
        "ax2.tick_params(axis='y', labelcolor=color)\n",
        "\n",
        "fig.tight_layout()\n",
        "plt.title(\"Gross Profit and Open Market Sourcing vs. Price\")\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "# Find the optimal price and corresponding open market percentage\n",
        "optimal_price_index = np.argmax(profits)\n",
        "optimal_price = prices[optimal_price_index]\n",
        "optimal_profit = profits[optimal_price_index]\n",
        "optimal_open_market_percentage = open_market_percentages[optimal_price_index]\n",
        "\n",
        "print(f\"Optimal Price: ${optimal_price:.2f}\")\n",
        "print(f\"Gross Profit at Optimal Price: ${optimal_profit}\")\n",
        "print(f\"Open Market Sourcing at Optimal Price: {optimal_open_market_percentage:.2f}%\")\n",
        "\n",
        "# --- Verification for $11 sales price ---\n",
        "(\n",
        "    open_market_percentage_11,\n",
        "    coop_percentage_11,\n",
        ") = calculate_sourcing_percentages(sales_price=11.0)\n",
        "print(\n",
        "    f\"\\nVerification at $11.00 sales price:\"\n",
        ")\n",
        "print(\n",
        "    f\"Percentage of cocoa sourced from open market: {open_market_percentage_11:.2f}%\"\n",
        ")\n",
        "print(\n",
        "    f\"Percentage of cocoa sourced from coop: {coop_percentage_11:.2f}%\"\n",
        ")"
      ]
    }
  ]
}